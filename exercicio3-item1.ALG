Algoritmo "infoUser"

Var
   nome: caractere
   idade: inteiro
   curso: caractere


Inicio

   escreva ("Qual seu nome? ")
   leia (nome)
   escreva ("Qual sua idade? ")
   leia (idade)
   escreva ("Em qual curso você está matriculado? ")
   leia (curso)

   Escreval ("Aluno: ", nome)
   Escreval ("Idade: ", idade)
   Escreval ("Matriculado no curso: ", curso)

Fimalgoritmo